{
  "name": "quiz-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "seed": "node scripts/seed.mjs",
    "prebuild": "cross-env NODE_ENV=production npm run seed",
    "build": "react-router build",
    "predev": "cross-env NODE_ENV=development npm run seed",
    "dev": "react-router dev",
    "start": "react-router-serve build/server/index.js",
    "format": "run-p format:biome format:prettier",
    "format:biome": "biome check --unsafe --write .",
    "format:prettier": "prettier --log-level silent --write \"**/*.md\"",
    "check": "run-p lint typecheck",
    "lint": "biome lint --write --unsafe .",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@uiw/codemirror-extensions-langs": "^4.23.7",
    "@uiw/codemirror-theme-github": "^4.23.7",
    "@uiw/react-codemirror": "^4.23.7",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "highlight.js": "^11.11.1",
    "isbot": "^5.1.28",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.1.0",
    "react-router": "7.6.1",
    "react-router-dom": "7.6.1",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0-beta.5",
    "@react-router/dev": "7.6.1",
    "@react-router/fs-routes": "7.6.1",
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/typography": "^0.5.16",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^22.15.27",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vercel/react-router": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "vite": "^5.4.19",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=20.18.0"
  }
}

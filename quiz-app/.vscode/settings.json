{
	"files.exclude": {
		"node_modules": true,
		"*.log": true,
		"dist": true,
		".env": true,
		".cache": true,
		"vite-cache": true,
		"*.tsbuildinfo": true,
		".react-router": true
	},
	"editor.defaultFormatter": "biomejs.biome",
	"[markdown]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.biome": "explicit",
		"source.organizeImports.biome": "explicit"
	}
}
